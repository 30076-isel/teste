<configuration>
    <system.web>
        <compilation batch="false"/>
		<customErrors mode="On">
		</customErrors>
    </system.web>
    <system.webServer>
        <handlers>
            <add name="iisnode" path="Server.js" verb="*" modules="iisnode" />
        </handlers>
        <iisnode loggingEnabled="false" />
    <rewrite>
        <rules>
            <rule name="myapp">
                <match url="/*" />
                <action type="Rewrite" url="Server.js" />
            </rule>
        </rules>
    </rewrite>
  </system.webServer>
</configuration>